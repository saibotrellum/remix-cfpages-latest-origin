{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "devbuild": "remix build --sourcemap",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "dev": "npm-run-all devbuild --parallel \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc",
    "wrangler": "wrangler pages dev ./public --persist",
    "deploy": "remix build && wrangler pages publish ./public --branch testing"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@remix-run/cloudflare": "^1.14.2",
    "@remix-run/cloudflare-pages": "^1.14.2",
    "@remix-run/react": "^1.14.2",
    "@remix-validated-form/with-zod": "^2.0.5",
    "@saibotsivad/mongodb": "^1.1.0",
    "bootstrap-icons": "^1.10.3",
    "cross-env": "^7.0.3",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "realm-web": "^2.0.0",
    "remix-validated-form": "^4.6.10",
    "remixicons": "^1.0.0",
    "zod-form-data": "^2.0.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@faker-js/faker": "^7.6.0",
    "@remix-run/dev": "^1.14.2",
    "@remix-run/eslint-config": "^1.14.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.8.4",
    "wrangler": "^2.2.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}
